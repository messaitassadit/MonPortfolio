{% extends 'base.html.twig' %}

{% block title %}
	AdminController!{{parent()}}
{% endblock %}

{% block body %}
	

	<div class="example-wrapper">
		{% for notification in app.flashes('success') %}
			<div>{{ notification }}</div>
		{% endfor %}
		

		{% for contact in contacts %}
			Bonjour
			{{ contact.firstname}}
			{{ contact.lastname }}
			{{ contact.comment}}
			
			<a href="{{ path('admin_supprimer_contact' , {'id' : contact.id} ) }}">supprimer</a><br>
		{% endfor %}
	{% endblock %}
